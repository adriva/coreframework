{
    "base": "basereport",
    "name": "promotions sample report",
    "contextProvider": "demo.Controllers.ReportContextProvider,demo",
    "commands": {
        "getTypeCodes": {
            "commandText": "System.TypeCode"
        },
        "getPromotions": {
            "commandText": "SELECT * FROM Promotion  WHERE Supplier = @supplier"
        }
    },
    "filters": {
        "supplier": {
            "displayName": "Dukkan",
            "dataType": "String",
            "defaultValue": "boyner.com.tr",
            "children": {
                "status": {
                    "displayName": "Durum",
                    "dataType": "Decimal"
                }
            }
        }
    },
    "output": {
        "command": "getPromotions",
        "dataSource": "DefaultSqlServer",
        "fields": {
            "Id": {
                "displayName": "Aydi",
                "options": {
                    "web": {
                        "formatter": "xx1",
                        "format": "C2"
                    }
                }
            },
            "name": {
                "displayName": "Name"
            },
            "Description": {}
        }
    }
}