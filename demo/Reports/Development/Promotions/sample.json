{
    "base": "basereport",
    "name": "promotions sample report",
    "contextProvider": "demo.Controllers.ReportContextProvider,demo",
    "commands": {
        "getTypeCodes": {
            "commandText": "System.TypeCode"
        },
        "getPromotions": {
            "commandText": "SELECT * FROM [Promotion] WHERE Supplier = @supplier"
        }
    },
    "filters": {
        "supplier": {
            "displayName": "Dukkan",
            "dataType": "DateTime",
            "properties": "context",
            "defaultValue": "GetCurrentDate",
            "children": {
                "status": {
                    "displayName": "Durum",
                    "dataType": "Decimal"
                }
            }
        }
    },
    "output": {
        "command": "getPromotions",
        "dataSource": "DefaultSqlServer"
    }
}