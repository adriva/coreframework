{
    "$schema": "../../report-schema.json",
    "base": "basereport",
    "commands": {
        "getRandomUsers": {
            "commandText": "/api/?nat=@nationalities&results=@pageSize",
            "options": {
                "dataElement": "results",
                "method": "GET"
            }
        }
    },
    "filters": {
        "nationalities": {
            "defaultValue": "us,dk,fr,gb",
            "dataType": "String"
        },
        "pageSize": {
            "defaultValue": 10,
            "dataType": "Int32"
        }
    },
    "output": {
        "command": "getRandomUsers",
        "dataSource": "randomUser",
        "fields": {
            "id.name": {
                "displayName": "Id"
            },
            "name.first": {
                "displayName": "First Name"
            },
            "name.last": {
                "displayName": "Last Name"
            }
        }
    }
}